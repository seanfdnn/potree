{
	"extends": "happiness",
	"rules": {
		"no-lone-blocks": "off",
		"space-before-function-paren": "off",
		"space-before-blocks": "off",
		"indent": "off",
		"no-trailing-spaces": "off",
		"keyword-spacing": "off",
		"no-multiple-empty-lines" : "off",
		"key-spacing": "off",
		"spaced-comment": "off",
		"quotes": "off",
		"space-in-parens": "off",
		"padded-blocks": "off",
		"comma-dangle": "off",
		"object-property-newline": "off",
		"eol-last": "off",
		"no-unused-vars": "off"
	},
	"parserOptions": { 
		"ecmaVersion": 11
	},
	"env": {
		"browser": true,
		"worker": true
	}
}
